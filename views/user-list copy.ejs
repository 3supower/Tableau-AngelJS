<!DOCTYPE html>
<html lang="en">
<head>
  <title>Fetch using MySQL and Node.js</title>
</head>
<body>
    <div class="table-data">
<h2>Display Data using Node.js & MySQL</h2>
    <table border="1">
        <tr>
            <th>S.N</th>
            <th>Full Name</th>
            <th>Email Address</th>
            <th>City</th>
            <th>Country</th>
            <th>Edit</th>
            <th>Delete</th>
        </tr>
        
        <%
        if(userData.length!=0){
            var i=1;
            // console.log(userData);
            userData.each(function(data){
                // console.log(data);
                console.log(data.CaseNumber);
                console.log('<h5>'+i+'</h5>');
        %>
        <tr>
            <td><%= i; %></td>
            <td><%= data.CaseNumber %></td>
            <td><%= data.Case_Owner_Name__c %></td>
            <td><%= data.Priority %></td>
            <td><%= data.Subject %></td>
            <td><a href="/users/edit/<%=data.Id%>">Edit</a></td>
            <td><a href="/users/delete/<%=data.Id%>">Delete</a></td>
        </tr>
        <%  i++; }) %>
        <% } else{ %>
            <tr>
                <td colspan="7">No Data Found</td>
            </tr>
        <% } %>
    </table>
    </div>
</body>
</html>